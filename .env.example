# Environment Variables Example
# Copy this file to .env and fill in your values

# MediaPipe Configuration
MEDIAPIPE_MODEL_COMPLEXITY=1
MEDIAPIPE_MIN_DETECTION_CONFIDENCE=0.5
MEDIAPIPE_MIN_TRACKING_CONFIDENCE=0.5

# Handrail Detection Parameters
HANDRAIL_TOUCH_THRESHOLD=30
HANDRAIL_MIN_LINE_LENGTH=120
HANDRAIL_HOUGH_THRESHOLD=60

# Video Processing
DEFAULT_OUTPUT_DIR=data/output
DEFAULT_INPUT_DIR=data/videos
MAX_VIDEO_RESOLUTION=1920x1080

# Logging
LOG_LEVEL=INFO
LOG_FILE=data/output/handrail_detection.log

# Performance
USE_GPU=false
MAX_PEOPLE_PER_FRAME=10
ENABLE_MOTION_DETECTION=true

# Analysis
GENERATE_SUMMARY_PLOTS=true
SAVE_ANALYSIS_JSON=true
ENABLE_REAL_TIME_DISPLAY=true